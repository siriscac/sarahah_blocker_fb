{
  "manifest_version": 2,
  "name": "Sarahah Fb Posts Blocker Plugin",
  "description": "This extension will block posts about Sarahah on Fb",
  "version": "1.0",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
     "activeTab", "tabs"
  ]
}
